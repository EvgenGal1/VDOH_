        <div class="DoC3" id="DoC3">
          <table>
            <!-- шапка -->
            <tr>
              <th>Название/Действие</th>
              <th>Описание/Альтернатива</th>
              <th>Пуск</th>
              <!-- /Настроки -->
            </tr>
            <!-- Выбор эл. -->
            <tr>
              <td colspan="3">Выбор элемента</td>
            </tr>
            <!-- выбор эл. -->
            <tr>
              <td>два способа</td>
              <td>
                <span class="token">
                  <span class="jq">$</span><span class="punct">(</span
                  ><span class="elem">''</span
                  ><span class="punct">)</span></span
                >
                <span> / </span>
                <span class="token">
                  <span class="jq">jquery</span><span class="punct">(</span
                  ><span class="elem">''</span
                  ><span class="punct">)</span></span
                >
              </td>
              <td>Пип</td>
            </tr>
            <!-- Выборка эл. -->
            <tr>
              <td colspan="3">Выборка элементов</td>
            </tr>
            <!-- базовые -->
            <tr>
              <td>
                Базовые: все / тег / класс / id / перечисление
                <!-- <div>тег / класс / id</div> -->
              </td>
              <td>
                <span class="token" title="выбор всех элементов">
                  <span class="jq">$</span><span class="punct">(</span
                  ><span class="elem">'*'</span
                  ><span class="punct">)</span></span
                >
                <span> / </span>
                <span class="token" title="выбор всех тегов (а)">
                  <span class="jq">$</span><span class="punct">(</span
                  ><span class="elem">'a'</span
                  ><span class="punct">)</span></span
                >
                <span> / </span>
                <span class="token" title="выбор класса (класс menu)">
                  <span class="jq">$</span><span class="punct">(</span
                  ><span class="elem">'.menu'</span
                  ><span class="punct">)</span></span
                >
                /
                <span class="token" title="выбор идентифокатора (id ul)">
                  <span class="jq">$</span><span class="punct">(</span
                  ><span class="elem">'#ul'</span
                  ><span class="punct">)</span></span
                >
                /
                <span
                  class="token"
                  title="выбор перечисленых эл. (тег a, класс .menu, id #ul)"
                >
                  <span class="jq">$</span><span class="punct">(</span
                  ><span class="elem">'a, .menu, #ul'</span
                  ><span class="punct">)</span></span
                >
              </td>
              <td>Пип</td>
            </tr>
            <!-- вложенность / дочерний / ближайший / следюущей -->
            <tr>
              <td>вложеность(потомок) / дочерний / ближайший / следюущей</td>
              <td>
                <span class="token" title="выбор всех потомков (все li)">
                  <span class="jq">$</span><span class="punct">(</span
                  ><span class="elem">'menu li'</span
                  ><span class="punct">)</span></span
                >
                <span> / </span>
                <span
                  class="token"
                  title="выбор только прямых потомков (сыновей)"
                >
                  <span class="jq">$</span><span class="punct">(</span
                  ><span class="elem">'menu > li'</span
                  ><span class="punct">)</span></span
                >
                <span> / </span>
                <span
                  class="token"
                  title="выбор ближайшего ниже (только первый <p>)"
                >
                  <span class="jq">$</span><span class="punct">(</span
                  ><span class="elem">'menu li a + p'</span
                  ><span class="punct">)</span></span
                >
                <span> / </span>
                <span
                  class="token"
                  title="выбор всех следующих (все дочерние <p>)"
                >
                  <span class="jq">$</span><span class="punct">(</span
                  ><span class="elem">'menu li a ~ p'</span
                  ><span class="punct">)</span></span
                >
              </td>
              <td>Пип</td>
            </tr>
            <!-- ближайший / следюущей -->
            <tr>
              <td>ближайший / следюущей</td>
              <td>
                <span
                  class="token"
                  title="выбор ближайшего ниже (только первый <p>)"
                >
                  <span class="jq">$</span><span class="punct">(</span
                  ><span class="elem">'menu li a + p'</span
                  ><span class="punct">)</span></span
                >
                <span> / </span>
                <span
                  class="token"
                  title="выбор всех следующих (все дочерние <p>)"
                >
                  <span class="jq">$</span><span class="punct">(</span
                  ><span class="elem">'menu li a ~ p'</span
                  ><span class="punct">)</span></span
                >
              </td>
              <td>Пип</td>
            </tr>
          </table>
